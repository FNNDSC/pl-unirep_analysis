{
  "type": "ds",
  "parameters": [
    {
      "name": "dimension",
      "type": "int",
      "optional": true,
      "flag": "--dimension",
      "short_flag": "-d",
      "action": "store",
      "help": "Dimension of the model",
      "default": 64,
      "ui_exposed": true
    },
    {
      "name": "batch_size",
      "type": "int",
      "optional": true,
      "flag": "--batch_size",
      "short_flag": "-b",
      "action": "store",
      "help": "Batch size of the model",
      "default": 12,
      "ui_exposed": true
    },
    {
      "name": "learning_rate",
      "type": "float",
      "optional": true,
      "flag": "--learning_rate",
      "short_flag": "-l",
      "action": "store",
      "help": "Learning rate of the training model",
      "default": 0.001,
      "ui_exposed": true
    },
    {
      "name": "inputFile",
      "type": "str",
      "optional": true,
      "flag": "--inputFile",
      "short_flag": "-i",
      "action": "store",
      "help": "Input file for the analysis",
      "default": "sequence.txt",
      "ui_exposed": true
    },
    {
      "name": "inputGlob",
      "type": "str",
      "optional": true,
      "flag": "--inputGlob",
      "short_flag": "-g",
      "action": "store",
      "help": "Input glob to find sequence text file",
      "default": "**/*.txt",
      "ui_exposed": true
    },
    {
      "name": "modelWeightPath",
      "type": "str",
      "optional": true,
      "flag": "--modelWeightPath",
      "short_flag": "-m",
      "action": "store",
      "help": "Directory containing weight files",
      "default": "/usr/local/lib/unirep_analysis/",
      "ui_exposed": true
    },
    {
      "name": "outputFile",
      "type": "str",
      "optional": true,
      "flag": "--outputFile",
      "short_flag": "-o",
      "action": "store",
      "help": "Output file for the analysis",
      "default": "format.txt",
      "ui_exposed": true
    }
  ],
  "icon": "",
  "authors": "FNNDSC <dev@babyMRI.org>",
  "title": "Unirep Analysis",
  "category": "Utility",
  "description": "A ChRIS plugin to run UniRep analysis",
  "documentation": "https://github.com/FNNDSC/pl-unirep_analysis",
  "license": "MIT",
  "version": "0.2.4",
  "selfpath": "/usr/local/bin",
  "selfexec": "unirep_analysis",
  "execshell": "/docker-entrypoint.sh",
  "min_number_of_workers": 1,
  "max_number_of_workers": 1,
  "min_memory_limit": 2000,
  "max_memory_limit": "",
  "min_cpu_limit": 2000,
  "max_cpu_limit": "",
  "min_gpu_limit": 0,
  "max_gpu_limit": 1
}
